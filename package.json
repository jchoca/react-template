{
  "name": "react-template",
  "version": "1.0.0",
  "description": "Browserify, Babel (6to5) & React example",
  "main": "gulpfile.js",
  "scripts": {
    "start": "gulp",
    "test": "mocha --compilers .:app/js/test/helpers/compiler.js app/js/test/*-test.js",
    "test-debug": "mocha --debug-brk --compilers .:app/js/helpers/compiler.js app/js/test/*-test.js",
    "lint": "jsxhint --es6module --reporter='node_modules/jshint-stylish' app/js/**/*.jsx"
  },
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.5.6",
    "babelify": "^6.1.2",
    "browser-sync": "^2.2.1",
    "browserify": "^8.1.3",
    "chai": "^3.0.0",
    "gulp": "^3.8.11",
    "gulp-sass": "^2.0.1",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-util": "^3.0.3",
    "jsdom": "^3.1.2",
    "jshint-stylish": "^2.0.0",
    "jsxhint": "^0.15.1",
    "lodash": "^3.9.3",
    "mocha": "^2.2.5",
    "node-inspector": "^0.10.1",
    "react": "^0.13.3",
    "react-tools": "^0.13.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.3.0"
  }
}
